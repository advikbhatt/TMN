                                        }, 1000);
                                    });
                                </script>
                            </div><!--counter_blocks-->
                        </div><!--col-md-6-->
                        <div id="counter_info_4"
                             class="col-6 d-flex align-items-center counter_info justify-content-center">
                            <div class="counter_blocks">
                                <h4 class="bni_counter_info">
                                    <span class="counter_prefix">$</span>
                                    <span id="counter_4" class="counter counter-default"
                                          data-thousand-delimiter=",">0</span>
                                    <span class="counter_suffix">B</span>
                                </h4>
                                <div class="bni_counter_text">
                                    <p>Member Generated Business</p>
                                </div><!--bni_counter_text-->
                                <script type="text/javascript">
                                    jQuery(document).ready(function () {
                                        setTimeout(function () {
                                            counterValue = 22.1;
                                            if (parseInt(counterValue) == counterValue) {
                                                decimalValue = 0;
                                            } else if (parseFloat(counterValue) == counterValue) {
                                                decimalValue = 1;
                                            }
                                            jQuery("#counter_4").countimator({
                                                value:22.1,
                                                duration: 2000,
                                                decimals: decimalValue,
                                                animateOnAppear: true,
                                                complete: function () {
                                                    jQuery('h4.counter_heading small').addClass("d-block");
                                                }
                                            });
                                        }, 1000);
                                    });
                                </script>
                            </div><!--counter_blocks-->
                        </div><!--col-md-6-->
                    </div><!--bniCounterNumberBlock-->
                </div><!--col-5-->
            </div><!--row-->

