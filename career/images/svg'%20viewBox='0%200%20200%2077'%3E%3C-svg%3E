<script>
    jQuery(function ($) {
        var searchRequest;
        var noData = "No results found";

        $('.search-autocomplete').autoComplete({
            minChars: 2,
            source: function (term, suggest) {
                try {
                    searchRequest.abort();
                } catch (e) {
                }
                searchRequest = $.post(global.ajax, {search: term, action: 'search_site'}, function (res) {
                    //console.log(res);
                    if (res != "") {
                        $(".searchSuggestions").removeClass('nodatafound').mCustomScrollbar("destroy");
                        $(".searchSuggestions").removeClass('nodatafound').html(res).show().mCustomScrollbar      ({
                            axis: "y"
                        });
                        $(".Searchajaxloader").removeClass('fad fa-spinner-third').addClass('fal fa-search');
                    } else {
                        $(".searchSuggestions").mCustomScrollbar("destroy").addClass('nodatafound');
                        $(".Searchajaxloader").removeClass('fad fa-spinner-third').addClass('fal fa-search');
                        $(".searchSuggestions").html(noData).show();
                    }
                });
            }
        });

        $(document).click(function (e) {
            $('.searchSuggestions').hide();
        });

        $('.search-autocomplete').on('keypress', function () {
            $(".Searchajaxloader").removeClass('fal fa-search').addClass('fad fa-spinner-third');
            $(".searchSuggestions").hide();
        });

        /* code for country code functionality */
        jQuery(window).load(function(){
            countryCode();
         });
    });
</script>

